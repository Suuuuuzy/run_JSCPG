{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 2,
    "name": "Brawl stars Cursor",
    "description": "Changes your cursor to a custom one.",
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "*://*/*"
    ],
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "*://*/*"
            ],
            "all_frames": true,
            "run_at": "document_start"
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "128": "icons/128.png"
    }
}