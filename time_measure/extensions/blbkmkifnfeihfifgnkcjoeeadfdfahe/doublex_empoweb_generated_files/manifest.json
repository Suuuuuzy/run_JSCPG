{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "NoteTab",
  "description": "Take notes in the New Tab Page",
  "version": "0.1",

  "permissions": [
    "storage"
  ],

  "chrome_url_overrides": {
    "newtab": "html/notes.html"
  },

  "icons": {
    "1": "NoteTab.png"
  },

  "browser_action": {
      "default_icon": "NoteTab.png"
  }
}
