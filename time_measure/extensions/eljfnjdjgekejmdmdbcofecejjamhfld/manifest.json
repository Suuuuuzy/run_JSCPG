{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "You're Hegemony",
    "description": "Let this be your plugin to easily identify who are hegemony.",
    "version":"0.1.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "options_page": "options.html",
    "permissions":["storage"],
    "icons": { "16": "rp16.png",
           "48": "rp48.png",
          "128": "rp128.png" }
}
