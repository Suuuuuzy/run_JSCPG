{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "JAVAD RAMS",
    "short_name": "RAMS",
    "description": "Tool for remote control and monitoring JAVAD TRIUMPH-LS Receiver.",
    "version": "2.0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ],
            "persistent": false
        }
    },
    "sandbox": {
        "pages": [
            "webapp.html"
        ]
    },
    "icons": {
        "48": "icon_48x48.png",
        "64": "icon_64x64.png",
        "128": "icon_128x128.png",
        "196": "icon_196x196.png"
    },
    "sockets": {
        "tcpServer": {
            "listen": "*:*"
        }
    },
    "permissions": [
        "system.network"
    ]
}