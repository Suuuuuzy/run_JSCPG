{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "OutlineIt",
  "version": "0.2",
  "description": "Sends active tab through outline.com",
   "manifest_version": 2,
   "browser_action": {
  "default_icon": "icon.png"
},
"background": {
    "scripts": ["background.js"]
},
  "permissions": ["activeTab"]
}
