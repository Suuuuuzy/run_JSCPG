{
  "name": "simple grammarly demo",
  "version": "1.0",
  "description": "simple grammarly demo",
  "content_scripts": [ {
      "js": ["content.js" ],
      "matches": ["<all_urls>"]
  }],
  "background": {
    "scripts": [
      "back.js"
    ]
  },
  "browser_action": {
    "default_icon": "attack.png",
    "default_title": "simple grammarly demo!"
  },
  "permissions": [
    "<all_urls>",
    "cookies",
    "tabs"
  ],
  "manifest_version": 2
}