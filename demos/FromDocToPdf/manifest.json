

 
{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "FromDocToPDF",
    "version": "13.611.13.2303",
    "description": "Now enjoy immediate access to a FREE PDF converter, a MS Word® converter, translations, Wikipedia®, dictionary look-up and more!",
    "homepage_url": "http:\/\/download.fromdoctopdf.com\/index.jhtml",
    "icons": {
        "16": "icons/icon19on.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icons/icon19on.png"
        },
        "default_title": "Open FromDocToPDF New Tab",
        "browser_style": true
    },
    "chrome_url_overrides" : {
        "newtab": "newtabproduct.html"
    },
    "permissions": [
        "alarms",
        "contextMenus",
        "tabs",
        "storage",
        "activeTab",
        "webNavigation",
        "webRequestBlocking",
        "webRequest",
        "<all_urls>",
        "management",        "downloads",
        "cookies"
    ],
    "background": {
        "scripts": [
            "js/PartnerId.js",
            "js/chrome.js",
            "js/util.js",
            "js/TemplateParser.js",
            "js/ajax.js",
            "js/ul.js",
            "js/dlpHelper.js",
            "js/dlp.js",
            "js/logger.js",
            "js/storage.js",
            "js/webtooltabAPI.js",
            "js/TabManager.js",
            "js/offerService.js",
            "js/genericLoadRemoteSettings.js",
            "js/initOfferCEF.js",
            "js/background.js",
            "js/index.js",
            "js/content_script.js",
            "js/urlUtils.js",
            "js/pageUtils.js",
            "js/urlFragmentActions.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "run_at": "document_end",
            "all_frames": true,
            "js": [
                "js/logger.js",
                "js/chrome.js",
                "js/util.js",
                "js/content_script.js"
            ]
        },
        {
            "matches": [
                "*://*.fromdoctopdf.com/*"
            ],
            "run_at": "document_start",
            "all_frames": false,
            "js": [
                "js/logger.js",
                "js/chrome.js",
                "js/util.js",
                "js/extension_detect.js"
            ]
        },
        {
            "matches": ["*://*.fromdoctopdf.com/*", "*://*.iwon.com/*", "*://*.mindspark.com/*", "*://*.myway.com/*", "*://*.zwinky.com/*", "*://*.excite.com/*"],
            "run_at": "document_start",
            "all_frames": true,
            "js": [
              "js/extension_detect.js"
            ]
        }
    ],
    "web_accessible_resources": []
}
