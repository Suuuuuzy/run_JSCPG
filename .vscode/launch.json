    "version": "0.2.0",
    "configurations": [
        {
            "name": "Object Graph Generator",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/generate_graph.py",
            "args": ["-p"],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Node.js: Current File, Current Location",
            "program": "${file}",
            "cwd": "${fileDirname}"
        },
        {
            "name": "Python: Current File, Current Location (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "enter-your-module-name-here",
            "console": "integratedTerminal"
        }
    ]
}