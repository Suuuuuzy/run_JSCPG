{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Eco Search",
  "description": "Eco Search New Tab",
  "version": "1.1",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "pages/index.html"
  },
  "permissions": [ "*://*.ecosearch.club/*", "webRequest", "webRequestBlocking", "storage", "cookies"  ],
  "content_scripts": [ {
    "js": [ "common/contentscript.js" ],
    "matches": ["*://*.ecosearch.club/*"],
    "run_at": "document_end"
  } ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Eco",
      "keyword": "https://www.ecosearch.club/search.aspx",
      "search_url": "https://search.ecosearch.club/chrome/newtab/search.aspx?q={searchTerms}&src=ds&pid=peco&v=1.1&gid=221",
      "suggest_url": "https://www.ecosearch.club/ntapi/suggestions/rd?q={searchTerms}",
      "search_url_post_params": "",
      "favicon_url": "https://www.ecosearch.club/img/search.png",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "background": {
    "scripts": [ "release/browseraction_release.js"]
  },
  "browser_action": {
    "default_icon": "img/logo_48x.png"
  },
  "icons": {
    "128": "img/logo_128x.png",
    "16": "img/logo_16x.png",
    "48": "img/logo_48x.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
