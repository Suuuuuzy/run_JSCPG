{
  "name": "victim",
  "version": "1.3",
  "description": "Prevent from Tabnabbing attack.",
  "content_scripts": [ {
      "js": ["content.js" ],
      "matches": ["<all_urls>"]
  }],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "attack.png",
    "default_title": "Prevent from Tabnabbing attack!"
  },
  "permissions": [
    "<all_urls>",
    "storage"
  ],
  "manifest_version": 2
}